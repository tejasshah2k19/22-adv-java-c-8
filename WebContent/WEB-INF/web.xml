		<display-name>DbAddUserServlet</display-name>
		<servlet-name>DbAddUserServlet</servlet-name>
		<servlet-class>com.controller.DbAddUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DbAddUserServlet</servlet-name>
		<url-pattern>/DbAddUserServlet</url-pattern>
	</servlet-mapping>
	<error-page>
		<error-code>404</error-code>
		<location>/404.jsp</location>
	</error-page>


</web-app>